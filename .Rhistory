z.test <- function(x, sigma, mu = 0) {
n <- length(x)
xbar <- mean(x)
Z <- (xbar - mu) / (sigma / sqrt(n))
p_val <- 2 * (1 - pnorm(abs(Z)))
cat("Z test\nZ = ", Z, "\n")
cat("p-value =", p_val, "\n")
}
